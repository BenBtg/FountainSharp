{
  "version": "1.0.0-*",
  "buildOptions": {
    "debugType": "portable",
    "emitEntryPoint": false,
    "compilerName": "fsc",
    "compile": {
      "includeFiles": [
        "../FountainSharp.Parse/Collections.fs",
        "../FountainSharp.Parse/StringParsing.fs",
        "../FountainSharp.Parse/FountainSyntax.fs",
        "../FountainSharp.Parse/FountainHelper.fs",
        "../FountainSharp.Parse/Utils.fs",
        "../FountainSharp.Parse/FountainParser.fs",
        "../FountainSharp.Parse/FountainDocument.fs",
        "../FountainSharp.Parse/HtmlFormatting.fs"
      ]
    },
    "outputName": "FountainSharp.Parse"
  },
  "tools": {
    "dotnet-compile-fsc": "1.0.0-preview2-*"
  },
  "dependencies": {
    "FSharp.Core": "4.0.1.7-alpha"
  },
  "frameworks": {
    "netstandard1.6": {
      "dependencies": {
        "NETStandard.Library": "1.6.0"
      }
    }
  }
}
